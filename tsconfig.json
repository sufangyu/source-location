{
  "exclude": ["**/*.spec.ts", "**/*.test.ts"],
  "compilerOptions": {
    "noImplicitThis": false,
    "outDir": "dist",
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["DOM", "ES2020"],
    "moduleResolution": "node",
    "strict": true,
    "declaration": false,
    "sourceMap": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "baseUrl": "./",
  }
}